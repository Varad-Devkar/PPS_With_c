#include <stdio.h>

int main{}
{
   int marks;

   printf("Enter marks\t:");
   scanf("%d", &marks);

   if(marks>100);
   {
   printf("Invalid marks");
   }
   else if(marks>=90)
   {
      printf("Excellent");
   }
   else if(marks>=80) 
   {
      printf("A");
   }
   else if(marks>=70)
   {
      printf("B");
   }
   else if(marks>=60)
   {
      printf("C");
   }
   else if(marks>=50)
   {
      printf("D");
   }
   else if(marks>=40)
   {
      printf("E");
   }
   else if(marks<40 &&marks>=0)
   {
      printf("FAIL");
   }
   else
   {
      printf("Invalid marks");
   }
   return 0;
}
