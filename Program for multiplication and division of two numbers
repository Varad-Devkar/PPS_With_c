#include <stdio.h>

int main()
{
    int num1, num2, mult;
    float fnum1, fnum2, div;

    printf("Enter first integer:\t");
    scanf("%d", &num1);
    printf("Enter second integer:\t");
    scanf("%d", &num2);
    mult = num1 * num2;
    printf("Multiplication of %d and %d is %d\n", num1, num2, mult);

    printf("Enter first float:\t");
    scanf("%f", &fnum1);
    printf("Enter second float:\t");
    scanf("%f", &fnum2);
    div = fnum1 / fnum2;
    printf("Division of %.2f and %.2f is %.2f\n", fnum1, fnum2, div);

    return 0;
}
