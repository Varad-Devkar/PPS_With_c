#include <stdio.h>

int main()
{
    int a, b;
    printf("Enter number a:\t");
    scanf("%d", &a);

    printf("\nEnter number b:\t");
    scanf("%d", &b);
    printf("Before swapping values of a and b are %d and %d\n", a, b);

    b = a + b;
    a = b - a;
    b = b - a;

    printf("\nAfter swapping values of a and b are %d and %d", a, b);
    return 0;
}
